{
  "version": "2.1.0-*",
  "description": "A key value pair storage to run on a client device",
  "authors": [ "Yufei Huang" ],
  "tags": [ "PCL", "Portable", "storage", "key", "value", "kv storage" ],

  "iconUrl": "https://avatars3.githubusercontent.com/u/9212229?s=64",
  "projectUrl": "https://github.com/studio-nine/Nine.Storage",
  "licenseUrl": "https://github.com/studio-nine/Nine.Storage/blob/master/LICENSE",

  "compile": [
    "../../Common/src/LazyAsync.cs",
    "../../Common/src/ObjectHelper.cs",
    "../../Common/src/CommonTasks.cs"
  ],

  "dependencies": {
    "Nine.Formatting.Abstractions": "1.5.0-*",
    "Nine.Storage.Abstractions": "2.1.0-*",
    "SQLite.Net-PCL": "3.1.1"
  },

  "frameworks": {
    "dotnet5.0": {
      "dependencies": {
        "System.IO.FileSystem": "4.0.0",
        "System.ObjectModel": "4.0.10",
        "System.Runtime": "4.0.20",
        "System.Threading.Tasks": "4.0.10"
      },
      "imports": "portable-net45+win8"
    },
    ".NETPortable,Version=v4.5,Profile=Profile7": {
      "frameworkAssemblies": {
        "Microsoft.CSharp": { "type": "build" },
        "System": { "type": "build" },
        "System.Collections": { "type": "build" },
        "System.Collections.Concurrent": { "type": "build" },
        "System.Core": { "type": "build" },
        "System.Diagnostics.Debug": { "type": "build" },
        "System.Globalization": { "type": "build" },
        "System.IO": { "type": "build" },
        "System.Linq": { "type": "build" },
        "System.ObjectModel": { "type": "build" },
        "System.Reflection": { "type": "build" },
        "System.Reflection.Extensions": { "type": "build" },
        "System.Runtime": { "type": "build" },
        "System.Runtime.Extensions": { "type": "build" },
        "System.Text.Encoding": { "type": "build" },
        "System.Text.Encoding.Extensions": { "type": "build" },
        "System.Threading": { "type": "build" },
        "System.Threading.Tasks": { "type": "build" }
      },
      "dependencies": {
        "PCLStorage": "1.0.2"
      },
      "compilationOptions": {
        "define": [ "PCL" ]
      }
    },
    "dnx451": {
      "frameworkAssemblies": {
        "System.Collections": "4.0.0",
        "System.Runtime": "4.0.10.0"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Collections": "4.0.0",
        "System.Runtime": "4.0.10.0"
      }
    }
